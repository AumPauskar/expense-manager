    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.4.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\ExpenseManagerApp.Application\ExpenseManagerApp.Application.csproj" />
    <ProjectReference Include="..\ExpenseManagerApp.Infrastructure\ExpenseManagerApp.Infrastructure.csproj" />
  </ItemGroup>

</Project>
